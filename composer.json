{
    "name": "onurrr/laravel-viacoinrpc",
    "type": "library",
    "description": "Viacoin JSON-RPC Service Provider for Laravel",
    "keywords": ["laravel","viacoin","api","jsonrpc","guzzle"],
    "homepage": "https://github.com/onurrr/laravel-viacoinrpc",
    "license": "MIT",
    "authors": [
        {
            "name": "Onurrr",
            "email": "mewnbot@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "onurrr/php-viacoinrpc": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.3.*",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4" : {
            "Onurrr\\Viacoin\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Onurrr\\Viacoin\\Providers\\ServiceProvider"
            ]
        }
    }
}
